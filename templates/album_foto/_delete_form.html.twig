<form method="post" action="{{ path('album_foto_delete', {'id': album_foto.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ album_foto.id) }}">
    <button class="btn btn-danger btn-sm">Delete</button>
</form>
